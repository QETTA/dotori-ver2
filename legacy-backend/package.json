{
  "name": "ipsoai",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "dev": "WATCHPACK_POLLING=true next dev",
    "dev:phone": "WATCHPACK_POLLING=true next dev --turbopack --hostname 0.0.0.0 --port 3000",
    "dev:high-mem": "set NODE_OPTIONS=--max-old-space-size=4096 && next dev",
    "build": "next build",
    "build:high-mem": "set NODE_OPTIONS=--max-old-space-size=4096 && next build",
    "start": "next start",
    "lint": "biome check ./src",
    "lint:fix": "biome check --write ./src",
    "format": "biome format --write ./src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:trend": "node ./scripts/run-playwright-local.cjs e2e/trend-mobile.spec.ts --project=MobileChrome",
    "test:e2e:trend:rapid": "node ./scripts/run-trend-rapid.cjs",
    "test:e2e:console": "node ./scripts/run-playwright-local.cjs e2e/console-regression.spec.ts --project=MobileChrome --workers=1",
    "test:e2e:console:rapid": "node ./scripts/run-console-rapid.cjs",
    "test:e2e:console:artifact": "node ./scripts/assert-console-artifact.cjs",
    "env:mongodb:check": "node ./scripts/assert-mongodb-env.cjs",
    "mcp:check": "node ./scripts/assert-mcp-stack.cjs",
    "mcp:playwright:check": "node ./scripts/assert-playwright-mcp.cjs",
    "test:e2e:hydration": "node ./scripts/run-hydration-check.cjs",
    "playwright:deps:user": "bash ./scripts/setup-playwright-user-deps.sh",
    "mobile:output:check": "npm run test:e2e:trend && npm run screenshots:all",
    "mobile:output:check:core": "npm run test:e2e:console && npm run screenshots:core",
    "workflow:cycle": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/workflow-cycle.ps1",
    "workflow:cycle:live": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/workflow-cycle.ps1 -RequireLiveAudit true",
    "workflow:strict": "npm run workflow:cycle:live && npm run review:ai:strict",
    "capability:audit:core": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/capability-audit-core.ps1 -BaseUrl http://localhost:3000 -RequireLive false",
    "capability:audit:core:live": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/capability-audit-core.ps1 -BaseUrl http://localhost:3000 -RequireLive true",
    "workflow:fast": "npm run workflow:cycle",
    "workflow:full": "npm run quality:full",
    "workflow:refactor": "npm run workflow:fast && npx playwright test e2e/chat-widget-intent.spec.ts && npm run workflow:full",
    "workflow:frontend:2026": "node ./scripts/frontend-2026-cycle.cjs --mode full",
    "workflow:frontend:2026:rapid": "node ./scripts/frontend-2026-cycle.cjs --mode rapid",
    "review:frontend:2026:triple": "node ./scripts/frontend-2026-triple-check.cjs --mode full",
    "review:frontend:2026:triple:rapid": "node ./scripts/frontend-2026-triple-check.cjs --mode rapid",
    "workflow:loop": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/start-auto-sync.ps1 -CheckScript workflow:cycle",
    "auto-sync:check": "npm run test:e2e:console && npm run test:e2e:trend",
    "review:ai": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/review-ai-safe.ps1 -TimeoutSec 900 -SoftFail true",
    "review:ai:strict": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/review-ai-safe.ps1 -TimeoutSec 900 -SoftFail false",
    "auto-sync:cycle": "npm run workflow:cycle && npm run review:ai",
    "quality:gate": "npm run typecheck && npm run lint:changed && npm run test:e2e:console && npm run screenshots:all",
    "quality:full": "npm run quality:gate && npm run test:e2e",
    "auto-sync:visual": "npm run workflow:cycle",
    "auto-sync": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/start-auto-sync.ps1 -CheckScript auto-sync:visual",
    "auto-sync:full": "npm run auto-sync",
    "auto-sync:daemon": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/auto-sync-daemon.ps1",
    "auto-sync:daemon:bg": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/auto-sync-daemon-start.ps1",
    "auto-sync:daemon:stop": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/auto-sync-daemon-stop.ps1",
    "playwright:sync": "node ./scripts/playwright-sync-worker.cjs",
    "playwright:sync:bg": "node ./scripts/playwright-sync-daemon-start.cjs --interval-sec 18 --health-path /home --request-timeout-ms 12000 --check-script test:e2e:console:rapid --mcp-check-script mcp:check --mcp-check-every 4",
    "playwright:sync:status": "node ./scripts/playwright-sync-daemon-status.cjs",
    "playwright:sync:stop": "node ./scripts/playwright-sync-daemon-stop.cjs",
    "dev:daemon:bg": "node ./scripts/dev-server-daemon-start.cjs --host localhost --port 3000 --health-path /home --boot-timeout-sec 120",
    "dev:daemon:status": "node ./scripts/dev-server-daemon-status.cjs",
    "dev:daemon:stop": "node ./scripts/dev-server-daemon-stop.cjs",
    "subagents:bg": "npm run dev:daemon:bg && npm run playwright:sync:bg",
    "subagents:status": "node ./scripts/subagent-stack-status.cjs",
    "subagents:stop": "npm run playwright:sync:stop && npm run dev:daemon:stop",
    "subagents5:bg": "node ./scripts/bg-5agents-start.cjs",
    "subagents5:status": "node ./scripts/bg-5agents-status.cjs",
    "subagents5:stop": "node ./scripts/bg-5agents-stop.cjs",
    "subagents5:watch": "node ./scripts/bg-5agents-watch.cjs 8",
    "test:e2e:ui": "playwright test --ui",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "analyze": "ANALYZE=true next build",
    "clean": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"if (Test-Path .next) { Remove-Item .next -Recurse -Force }; if (Test-Path node_modules/.cache) { Remove-Item node_modules/.cache -Recurse -Force }\"",
    "clean:polish": "node ./scripts/polish-cleanup.cjs",
    "dev:recover": "npm run clean && npm run dev -- --port 3000 --hostname localhost",
    "db:push": "prisma db push",
    "db:migrate": "npm run db:push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o storybook-static",
    "codex:switch52": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-model-switch.ps1 -Model 52",
    "codex:switch53": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-model-switch.ps1 -Model 53",
    "codex:resume52": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-model-switch.ps1 -Model 52 -Mode resume",
    "codex:resume53": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-model-switch.ps1 -Model 53 -Mode resume",
    "codex:fork53": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-model-switch.ps1 -Model 53 -Mode fork",
    "codex:fork52": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-model-switch.ps1 -Model 52 -Mode fork",
    "codex:max": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-tier-switch.ps1 -Tier max",
    "codex:stable": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-tier-switch.ps1 -Tier stable",
    "codex:resume:max": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-tier-switch.ps1 -Tier max -Mode resume",
    "codex:resume:stable": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-tier-switch.ps1 -Tier stable -Mode resume",
    "codex:fork:max": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-tier-switch.ps1 -Tier max -Mode fork",
    "codex:fork:stable": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-tier-switch.ps1 -Tier stable -Mode fork",
    "codex:ab": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-ab.ps1",
    "codex:safe": "node ./scripts/codex-safe-exec.cjs",
    "codex:batch": "node ./scripts/codex-batch-runner.cjs",
    "codex:batch:dry": "node ./scripts/codex-batch-runner.cjs --dry-run",
    "codex:report": "node ./scripts/codex-pipeline-report.cjs",
    "codex:report:json": "node ./scripts/codex-pipeline-report.cjs --json",
    "codex:monitor": "node ./scripts/codex-monitor.cjs",
    "codex:spec:validate": "node ./scripts/codex-spec-validate.cjs",
    "codex:spec:validate:all": "node ./scripts/codex-spec-validate.cjs --all",
    "codex:tier": "bash ./scripts/codex-tier-switch.sh",
    "codex:ab:bash": "bash ./scripts/codex-ab.sh",
    "lint:changed": "node ./scripts/lint-changed.cjs",
    "lint:baseline": "node ./scripts/lint-changed.cjs --reset-baseline",
    "codex:auto": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-autopilot.ps1",
    "codex:auto:new": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-autopilot.ps1 -Mode new",
    "codex:auto:resume": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-autopilot.ps1 -Mode resume",
    "codex:auto:fork": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/codex-autopilot.ps1 -Mode fork",
    "screenshots:all": "node ./scripts/screenshot-all-pages.cjs",
    "screenshots:core": "SCREENSHOT_ROUTE_MODE=core SCREENSHOT_MODES=public node ./scripts/screenshot-all-pages.cjs"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.0",
    "@prisma/client": "^6.0.0",
    "@tanstack/react-query": "^5.60.0",
    "clsx": "^2.1.0",
    "critters": "^0.0.23",
    "lucide-react": "^0.574.0",
    "mongodb": "^7.1.0",
    "motion": "^12.0.0",
    "next": "^15.1.0",
    "next-auth": "^5.0.0-beta.25",
    "next-safe-action": "^8.0.0",
    "pretendard": "^1.3.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "web-push": "^3.6.7",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@playwright/test": "^1.48.0",
    "@storybook/addon-a11y": "^10.0.0",
    "@storybook/addon-essentials": "^8.4.0",
    "@storybook/nextjs": "^10.0.0",
    "@storybook/react": "^10.0.0",
    "@tailwindcss/postcss": "^4.1.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/node": "^25.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/web-push": "^3.6.4",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.0",
    "@vitest/ui": "^4.0.0",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.0",
    "prisma": "^6.0.0",
    "storybook": "^10.0.0",
    "tailwindcss": "^4.1.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
