{
  "name": "dotori-app",
  "version": "0.1.0",
  "private": true,
  "description": "도토리 — 어린이집·유치원 통합 매칭 플랫폼",
  "repository": {
    "type": "git",
    "url": "https://github.com/QETTA/dotori-ver2.git"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "bash scripts/dev/start-dotori-3000.sh",
    "dev:3000": "bash scripts/dev/start-dotori-3000.sh",
    "dev:kill-3000": "bash scripts/dev/kill-legacy-3000.sh",
    "dev:3002": "next dev --turbopack --port 3002",
    "build": "env -u NODE_ENV next build",
    "build:ci": "SKIP_ENV_VALIDATION=1 next build",
    "start": "next start",
    "start:3002": "next start --port 3002",
    "lint": "eslint",
    "lint:style-guard": "bash scripts/style-guard.sh",
    "typecheck": "tsc --noEmit",
    "ci": "npm run format:check && npm run ci:preflight && npm run build:ci",
    "ci:preflight": "npm run lint && npm run lint:style-guard && npm run check:ds-delta && npm run typecheck && npm test",
    "deploy:do:source": "bash ../scripts/deploy-do-local-fast.sh",
    "format": "prettier --write 'src/**/*.{ts,tsx}' 'scripts/**/*.{ts,mjs}' README.md package.json",
    "format:check": "prettier --check 'src/**/*.{ts,tsx}' 'scripts/**/*.{ts,mjs}' README.md package.json",
    "screenshot": "npx tsx scripts/screenshot-check.ts",
    "gg:trigger": "bash scripts/gg-trigger.sh",
    "gg:trigger:fast": "GG_MODE=fast bash scripts/gg-trigger.sh",
    "gg:trigger:full": "GG_MODE=full bash scripts/gg-trigger.sh",
    "gg:trigger:auto": "GG_MODE=auto bash scripts/gg-trigger.sh",
    "gg:trigger:adaptive": "GG_MODE=auto GG_ADAPTIVE_ORCHESTRATION=1 bash scripts/gg-trigger.sh",
    "gg:trigger:quality": "GG_MODE=auto SCREENSHOT_QUALITY_RETRY_COUNT=2 SCREENSHOT_QUALITY_RETRY_WAIT_MS=1500 bash scripts/gg-trigger.sh",
    "gg:trigger:stable": "GG_MODE=fast GG_START_SERVER=0 GG_SELF_GROWTH=0 GG_ADAPTIVE_ORCHESTRATION=0 GG_MULTI_AGENT=0 GG_RUN_UX_GUARD=0 GG_RUN_CONSOLE_GUARD=1 GG_CONSOLE_BROWSER=chromium GG_CONSOLE_CHANNEL=chrome GG_SCREENSHOT_PASSES=1 SCREENSHOT_ROUTE_LIMIT=25 SCREENSHOT_NAV_RETRIES=1 SCREENSHOT_MAX_ROUTE_FAILURES=0 SCREENSHOT_QUALITY_RETRY_COUNT=2 SCREENSHOT_QUALITY_RETRY_WAIT_MS=1500 bash scripts/gg-trigger.sh",
    "gg:trigger:reset": "GG_SELF_GROWTH_RESET=1 bash scripts/gg-trigger.sh",
    "gg:trigger:growth-off": "GG_SELF_GROWTH=0 bash scripts/gg-trigger.sh",
    "ux:guard": "bash scripts/ux-guard.sh",
    "qa:mobile": "./scripts/mobile-qa.sh",
    "check-console": "npx tsx scripts/check-console.ts",
    "check-console:chrome": "BROWSER=chromium CHECK_CONSOLE_CHANNEL=chrome npx tsx scripts/check-console.ts",
    "check:ds": "npx tsx scripts/check-design-system.ts",
    "check:ds-delta": "DS_CHANGED_FILES=\"${DS_CHANGED_FILES:-}\" npx tsx scripts/ds-delta-guard.ts",
    "audit:design": "DS_REPORT=1 npx tsx scripts/check-design-system.ts",
    "audit:design:strict": "DS_REPORT=1 DS_SCORE_ENFORCE=1 DS_SCORE_MIN=85 npx tsx scripts/check-design-system.ts",
    "audit:mobile": "npx tsx scripts/check-mobile-integrity.ts",
    "audit:mobile:strict": "MOBILE_SCORE_ENFORCE=1 MOBILE_SCORE_MIN=90 npx tsx scripts/check-mobile-integrity.ts",
    "audit:design:all": "DS_REPORT=1 DS_AUDIT_ALL=1 npx tsx scripts/check-design-system.ts",
    "audit:design:strict-all": "DS_REPORT=1 DS_AUDIT_ALL=1 DS_RAW_CLASS_MAX=0 DS_SCORE_ENFORCE=1 DS_SCORE_MIN=97 npx tsx scripts/check-design-system.ts",
    "multi-agent:next-wave": "bash scripts/multi-agent-next-wave.sh",
    "multi-agent:next-wave:wide": "WAVE_SIZE=6 bash scripts/multi-agent-next-wave.sh",
    "multi-agent:overdrive": "bash scripts/multi-agent-overdrive.sh",
    "multi-agent:overdrive:max": "WAVE_SIZE=8 bash scripts/multi-agent-overdrive.sh",
    "clean": "rm -rf .next node_modules/.cache",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:engine": "vitest run src/lib/engine/__tests__",
    "test:integration": "vitest run src/__tests__/api",
    "test:engine:coverage": "vitest run src/lib/engine/__tests__ --coverage.enabled --coverage.provider=v8 --coverage.reportsDirectory=coverage/engine --coverage.reporter=text --coverage.reporter=json-summary --coverage.thresholds.lines=60 --coverage.thresholds.functions=70 --coverage.thresholds.statements=60 --coverage.thresholds.branches=45",
    "test:engine:cov": "npm run test:engine:coverage",
    "test:engine:flaky": "node scripts/flaky-engine-tests.mjs",
    "test:e2e": "playwright test",
    "optimize:svg": "npx tsx scripts/optimize-svgs.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@auth/mongodb-adapter": "^3.11.1",
    "@formkit/auto-animate": "0.9.0",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "clsx": "^2.1.1",
    "culori": "4.0.2",
    "lucide-react": "0.575.0",
    "mongodb": "^6.21.0",
    "mongoose": "^8.23.0",
    "motion": "^12.34.2",
    "next": "16.1.6",
    "next-auth": "5.0.0-beta.30",
    "pdf-lib": "^1.17.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.5.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/culori": "4.0.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "svgo": "4.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
